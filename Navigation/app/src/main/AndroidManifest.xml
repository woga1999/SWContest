<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.woga1.navigation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoadingActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".MenuActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".SearchActivity"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".ReadyActivity"
            android:parentActivityName=".MenuActivity" />
        <activity android:name=".NavigationActivity" />
        <activity android:name=".BackActivity" />
        <activity android:name=".FavoriteActivity" />
        <activity android:name=".NearActivity" />
        <activity android:name=".DestinationActivity" />
        <activity
            android:name=".StopNavigation"
            android:theme="@android:style/Theme.DeviceDefault.Light.Dialog"></activity>
        <activity android:name=".DetailSearchActivity"></activity>
    </application>

</manifest>